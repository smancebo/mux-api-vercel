
{
  "compilerOptions": {
    "target": "esnext",                      
    "module": "commonjs",                    
    "lib": ["es6"],                           
    "allowJs": true,                          
    "noEmit": true,                           
    "isolatedModules": true,                  

    "strict": true,                           
    "moduleResolution": "node",               
    "baseUrl": "./api",                          
    "paths": {
      
    },                           
    "allowSyntheticDefaultImports": true,     
    "esModuleInterop": true               
  },
  "exclude": [
    "node_modules", "babel.config.js", "metro.config.js", "jest.config.js"
  ]
}
